#!/bin/bash
newman run ACI-Devnet-Demo.postman_collection.json --insecure -e ACI-Devnet-Demo-Env.json --folder Discover_Fabric -d discover-fabric.csv && newman run ACI-Devnet-Demo.postman_collection.json --insecure -e ACI-Devnet-Demo-Env.json --folder BGP -d bgp.csv && newman run ACI-Devnet-Demo.postman_collection.json --insecure -e ACI-Devnet-Demo-Env.json --folder Common-Tenant -d common-tenant.csv && newman run ACI-Devnet-Demo.postman_collection.json --insecure -e ACI-Devnet-Demo-Env.json --folder VMwareVMMDomain -d vmm-domain.csv && newman run ACI-Devnet-Demo.postman_collection.json --insecure -e ACI-Devnet-Demo-Env.json --folder VPCProfiles -d vpc-profile.csv && newman run ACI-Devnet-Demo.postman_collection.json --insecure -e ACI-Devnet-Demo-Env.json --folder VlanPools -d vlan-pool.csv && newman run ACI-Devnet-Demo.postman_collection.json --insecure -e ACI-Devnet-Demo-Env.json --folder Tenants -d tenants.csv
